let map f n =
  let rec loop n =
    fun acc ->
      if n <= 0 then acc
      else (loop (n - 1)) (acc + f n)
  in
  (loop n) 0
in
let fold f n init =
  let rec loop n =
    fun acc ->
      if n <= 0 then acc
      else (loop (n - 1)) (f acc n)
  in
  (loop n) init
in
let inc x = x + 1 in
let add a b = a + b in
let mapped = map inc 50 in
fold add 50 mapped
